                        },
                        {
                          "type": "text",
                          "id": "WUFVI",
                          "name": "code2l2",
                          "fill": "#555555",
                          "content": "    salience = compute_salience(stimulus)",
                          "lineHeight": 1.6,
                          "fontFamily": "IBM Plex Mono",
