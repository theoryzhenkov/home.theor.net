                        },
                        {
                          "type": "text",
                          "id": "ORyG1",
                          "name": "codeLine1",
                          "fill": "#E7E5E4",
                          "content": "def attend(stimulus, threshold=0.5):",
                          "lineHeight": 1.6,
                          "fontFamily": "JetBrains Mono",
                          "fontSize": 13,
                          "fontWeight": "normal"
                        },
                        {
                          "type": "text",
                          "id": "qASQz",
                          "name": "codeLine2",
                          "fill": "#A8A29E",
                          "content": "    salience = compute_salience(stimulus)",
                          "lineHeight": 1.6,
                          "fontFamily": "JetBrains Mono",
                          "fontSize": 13,
                          "fontWeight": "normal"
                        },
                        {
                          "type": "text",
                          "id": "HeCmQ",
                          "name": "codeLine3",
                          "fill": "#E7E5E4",
                          "content": "    if salience > threshold:",
                          "lineHeight": 1.6,
                          "fontFamily": "JetBrains Mono",
                          "fontSize": 13,
                          "fontWeight": "normal"
                        },
                        {
                          "type": "text",
                          "id": "v9a96",
                          "name": "codeLine4",
                          "fill": "#D6D3D1",
                          "content": "        return allocate_resources(stimulus)",
                          "lineHeight": 1.6,
                          "fontFamily": "JetBrains Mono",
                          "fontSize": 13,
                          "fontWeight": "normal"
                        }
                      ]
                    },
                    {
                      "type": "text",
                      "id": "AVCAl",
                      "name": "p5",
                      "fill": "#1C1917",
                      "textGrowth": "fixed-width",
                      "width": "fill_container",
                      "content": "This multi-level selection creates what Lavie calls perceptual load theory⁴ — the idea that attentional selection depends on the perceptual demands of the task at hand. When perceptual load is high, irrelevant distractors are effectively filtered out.",
                      "lineHeight": 1.65,
                      "fontFamily": "EB Garamond",
                      "fontSize": 17,
                      "fontWeight": "normal"
                    }
                  ]
                }
